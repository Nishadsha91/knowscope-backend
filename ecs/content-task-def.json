{
  "family": "content-service",
  "executionRoleArn": "arn:aws:iam::486403856118:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "3072",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "containerDefinitions": [
    {
      "name": "content-service-container",
      "image": "486403856118.dkr.ecr.ap-south-1.amazonaws.com/knowscope/content-service",
      "essential": true,
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 8000,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "MONGO_URI",
          "value": "mongodb+srv://knowscope80_db_user:eFNcYt5zVvRL9qAS@knowscope.djnzjt4.mongodb.net/?appName=knowscope"
        },
        {
          "name": "MONGO_DB",
          "value": "knowscope"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/content-service",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}